{
  "arrowParens": "avoid",
  "semi": false,
  "tabWidth": 2,
  "printWidth": 80,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "pluginSearchDirs": ["./node_modules"],
  "plugins": [
    "./node_modules/prettier-plugin-astro",
    "./node_modules/prettier-plugin-tailwindcss"
  ]
}
