    "@astrojs/tailwind": "^3.1.2",
    "@fontsource/jetbrains-mono": "^5.0.3",
    "@markdoc/markdoc": "^0.3.0",
    "@resvg/resvg-js": "^2.4.1",
    "@tailwindcss/typography": "^0.5.9",
    "astro": "^2.8.5",
    "astro-icon": "^0.8.1",
