    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.5",
    "add": "^2.0.6",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
